              <label for="remember" class="text-gray-600">Remember me</label>
            </div>
            <div>
              <a href="/register" class="hover:text-red-400">
                Don't have an account?
              </a>
            </div>
          </div>

          <button type="submit" class="btn-primary block mx-auto text-white text-sm uppercase rounded-full shadow-md px-6 py-2">Log
            in</button>
        </div>
      </form>
    </div>
